function run_get_gc(opts) {
	var dsgid = opts.dsgid;
	var div1 = $('#'+opts.divid1);
	var div2 = $('#'+opts.divid2);
	var div3 = $('#'+opts.divid3);
	var div4 = $('#'+opts.divid4);

	var busy = '<img src="picts/ajax-loader.gif"/>';
	div1.html(busy);
	div2.html(busy);
	div3.html(busy);
	div4.html(busy);

	$.ajax({
	    data: {
	 	    jquery_ajax: 1,
	       	fname: 'get_gc',
	       	dsgid: dsgid
	    },
		success: function(data) {
			var items = data.split('_');
		    div1.html(items[0]);
		    div2.html(items[1]);
		    div3.html(items[2]);
		    div4.html(items[3]);
		}
	});
}

